{
	"name": "@fleax/ui",
	"version": "0.1.0",
	"description": "Optional UI components for Fleax",
	"license": "MIT",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"sideEffects": true,
	"exports": {
		".": "./dist/index.js",
		"./styles.css": "./styles.css",
		"./islands/*": "./dist/islands/*.js"
	},
	"files": [
		"dist",
		"styles.css"
	],
	"scripts": {
		"build": "tsc -p tsconfig.json && node scripts/copy-css.mjs",
		"watch": "node scripts/watch.mjs",
		"typecheck": "tsc -p tsconfig.json --noEmit"
	},
	"devDependencies": {
		"@fleax/core": "file:../.."
	},
	"peerDependencies": {
		"@fleax/core": "^1.1.0"
	}
}
